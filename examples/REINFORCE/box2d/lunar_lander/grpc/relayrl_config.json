        "TD3": {
            "seed": 1,
            "gamma": 0.99,
            "tau": 0.005,
            "learning_rate": 3e-4,
            "batch_size": 128,
            "buffer_size": 50000,
            "exploration_noise": 0.1,
            "policy_noise": 0.2,
            "noise_clip": 0.5,
            "learning_starts": 25000,
            "policy_frequency": 2
        }
