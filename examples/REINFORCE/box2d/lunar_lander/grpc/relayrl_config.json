{
    "algorithms": {
        "C51": {
            "batch_size": 64,
            "act_dim": 4,
            "seed": 0,
            "traj_per_epoch": 5,
            "n_atoms": 51,
            "v_min": -500,
            "v_max": 1000,
            "gamma": 0.95,
            "epsilon": 1.0,
            "epsilon_min": 0.01,
            "epsilon_decay": 0.999,
            "train_update_freq": 4,
            "target_update_freq": 50,
            "q_lr": 1e-5,
            "train_q_iters": 50
        },
        "DDPG": {
            "seed": 1,
            "gamma": 0.99,
            "tau": 1e-2,
            "learning_rate": 3e-3,
            "batch_size": 128,
            "buffer_size": 50000,
            "learning_starts": 128,
            "policy_frequency": 1,
            "noise_scale": 0.1,
            "train_iters": 50
        },
        "DQN": {
            "batch_size": 32,
            "seed": 0,
            "traj_per_epoch": 3,
            "gamma": 0.95,
            "epsilon": 1.0,
